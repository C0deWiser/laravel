{
    "name": "codewiser/laravel",
    "description": "Packages I might include to every Laravel application",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "pm",
            "email": "pm@101media.ru"
        }
    ],
    "require": {
        "php": "^8.1",
        "codewiser/dadata": "^1.0",
        "codewiser/http-cache-control": "^2.0",
        "codewiser/laravel-cast-datetime-tz": "^1.0",
        "codewiser/laravel-meilisearch": "^1.0",
        "codewiser/laravel-notifications": "^1.0",
        "codewiser/laravel-sendsay-mailer": "^1.0",
        "codewiser/workflow": "^4.4",
        "codewiser/socialiteprovider": "^1.0",
        "codewiser/laravel-make": "^1.0",
        "codewiser/telegram-channel": "^1.0",
        "codewiser/intl": "^1.0",
        "codewiser/laravel-scout": "^1.0",
        "codewiser/laravel-smscenter-mailer": "^1.0",
        "cerbero/enum": "^2.1",
        "codewiser/simple-files": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Codewiser\\": "src/"
        },
        "files" : [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4"
    },
    "repositories": [{
        "type": "github",
        "url": "https://github.com/C0deWiser/simple-storage.git"
    }]
}
